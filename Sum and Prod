
# Qs Link --> https://www.hackerrank.com/challenges/np-sum-and-prod/problem?h_r=next-challenge&h_v=zen&h_r=next-challenge&h_v=zen&h_r=next-challenge&h_v=zen

import numpy as np
n, m = map(int, input().split())
a = np.array([input().split() for _ in range(n)], int)
print(np.prod(np.sum(a, axis = 0), axis = 0))
